# Inbox Auto-Processing Flags (for build and runtime)
REACT_APP_INBOX_AUTO_ROUTE=1
REACT_APP_INBOX_AUTO_OCR=1
REACT_APP_BANK_IMPORT_ENABLE=1

# OCR Configuration
REACT_APP_OCR_CONFIDENCE_THRESHOLD=0.80

# Document AI Configuration (for Netlify functions)
DOC_AI_PROJECT_ID=your-project-id
DOC_AI_LOCATION=eu
DOC_AI_PROCESSOR_ID=your-processor-id
DOC_AI_SA_JSON_B64=your-service-account-key-base64

# Development flags
REACT_APP_ENABLE_TELEMETRY=1
REACT_APP_QA_DASHBOARD=1